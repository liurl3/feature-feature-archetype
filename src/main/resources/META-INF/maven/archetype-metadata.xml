<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="demo-manager"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <requiredProperties>
	<requiredProperty key="featureName" >
      <defaultValue>Digiwes Feature name</defaultValue>
    </requiredProperty>
      <requiredProperty key="featureDescription" >
          <defaultValue>Digiwes Feature description</defaultValue>
      </requiredProperty>
      <requiredProperty key="featureId" >
          <defaultValue>${artifactId.replace(".feature","")}</defaultValue>
      </requiredProperty>
  </requiredProperties>
</archetype-descriptor>
